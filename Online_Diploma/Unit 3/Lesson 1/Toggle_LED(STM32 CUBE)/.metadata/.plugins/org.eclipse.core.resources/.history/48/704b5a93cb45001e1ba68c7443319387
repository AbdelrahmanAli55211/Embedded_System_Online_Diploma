#include "stdint.h"
#define RCC_BASE 0x40021000
#define GPIO_BASE 0x40010800
#define APP2ENR_REG *((uint32_t *) (RCC_BASE+0x18))
#define CRH_REG *((uint32_t *) (GPIO_BASE+0x04))
#define ODR_REG *((uint32_t *) (GPIO_BASE+0x0c))
int main(void)
{
	APP2ENR_REG |= (1<<2);
	CRH_REG &= 0xff0fffff;
	CRH_REG |= 0x00200000;
    /* Loop forever */
	while(1)
	{
		ODR_REG |= (1<<13);//set bit
		for(int i=0;i<5000;i++);//delay
		ODR_REG &=~(1<<13);//clear bit
	}
	return 0;
}
